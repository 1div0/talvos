MODULE vecadd.spv
ENTRY vecadd

ALLOCATE a BUFFER 64 RANGE INT32 0 1
ALLOCATE b BUFFER 64 FILL  INT32 7
ALLOCATE c BUFFER 64 FILL  INT32 0

DESCRIPTOR_SET 0 0 a
DESCRIPTOR_SET 0 1 b
DESCRIPTOR_SET 0 2 c

DISPATCH 16 1 1

DUMP INT32 c

